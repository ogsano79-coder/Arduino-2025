int on=10,onbir=11,oniki=12,onuc=13;
void setup()
{
  pinMode(on ,OUTPUT);
  pinMode(onbir ,OUTPUT);
  pinMode(oniki ,OUTPUT);
  pinMode(onuc ,OUTPUT);
}

void loop()
{
  digitalWrite(on, HIGH);
  delay(100);
  digitalWrite(on, LOW);
  delay(100);
  
  
  digitalWrite(onbir, HIGH);
  delay(100);
  digitalWrite(onbir, LOW);
  delay(100);
  
  digitalWrite(oniki, HIGH);
  delay(100);
  digitalWrite(oniki, LOW);
  delay(100);
  
  
  digitalWrite(onuc, HIGH);
  delay(100);
  digitalWrite(onuc, LOW);
  delay(100);
  
  
  
  
  
  
  
  
  
  
}
